{
  "profiles": {
    "Development": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:7901",
      "launchUrl": "http://localhost:7901/swagger"
    },
    "Production": {
      "commandName": "Executable",
      "executablePath": "powershell.exe",
      "commandLineArgs": "-ExecutionPolicy Bypass -File \"$(ProjectDir)..\\run-prod.ps1\"",
      "workingDirectory": "$(ProjectDir)..",
      "launchBrowser": true,
      "launchUrl": "http://localhost:7900/swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production"
      }
    }
  }
}